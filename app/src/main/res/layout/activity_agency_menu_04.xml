<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/login_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="40dp">

        <ImageView
            android:layout_width="16.7dp"
            android:layout_height="16.7dp"
            android:layout_marginLeft="12.7dp"
            android:layout_marginTop="15.3dp"
            android:src="@drawable/index_item" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_title"
            android:textColor="#16133e"
            android:textSize="18.7sp" />
    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <TextView
            android:id="@+id/tv_agency_04_activity_title_order_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_trans_date_title"
            android:textColor="#61607b"
            android:textSize="13.3sp" />
        <RelativeLayout
            android:id="@+id/rl_agency_04_start_order_date"
            android:layout_width="123dp"
            android:layout_height="26.3dp"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/tv_agency_04_activity_title_order_date"
            android:layout_marginLeft="20dp"
            android:background="@drawable/calendar_selector">
            <TextView
                android:id="@+id/tv_agency_04_order_date"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textSize="12.7sp"
                android:textColor="#4564e8"
                android:text="2019/09/26"/>

            <ImageView
                android:layout_width="16.7dp"
                android:layout_height="16.7dp"
                android:layout_marginRight="10dp"

                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/calendar_bt" />
        </RelativeLayout>

        <ImageView
            android:layout_toRightOf="@+id/rl_agency_04_start_order_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:src="@drawable/s"
            android:layout_marginLeft="10dp"/>

        <RelativeLayout
            android:id="@+id/rl_agency_04_end_order_date"
            android:layout_width="123dp"
            android:layout_height="26.3dp"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true"
            android:layout_marginLeft="40dp"
            android:layout_marginRight="17dp"
            android:background="@drawable/calendar_selector">
            <TextView
                android:id="@+id/tv_agency_04_order_end_date"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginRight="17dp"
                android:layout_gravity="center_vertical"
                android:gravity="left|center_vertical"
                android:paddingLeft="10dp"
                android:textSize="12.7sp"
                android:textColor="#4564e8"
                android:text="2019/09/26"/>

            <ImageView
                android:layout_width="16.7dp"
                android:layout_height="16.7dp"
                android:layout_marginRight="10dp"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:src="@drawable/calendar_bt" />
        </RelativeLayout>

    </RelativeLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <Spinner
            android:id="@+id/sp_agency_04_activity_trans_type"
            android:layout_alignParentRight="true"
            android:layout_marginRight="17dp"
            android:spinnerMode="dropdown"
            android:layout_width="150dp"
            android:layout_height="34dp" />

        <TextView
            android:id="@+id/tv_agency_04_activity_trans_type_title"
            android:layout_toLeftOf="@+id/sp_agency_04_activity_trans_type"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="34dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_trans_type_title"
            android:textColor="#61607b"
            android:textSize="13.3sp" />
    </RelativeLayout>
<!--android:layout_toLeftOf="@+id/sp_agency_04_activity_cus_sms" -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp">

        <TextView
            android:id="@+id/tv_agency_04_activity_cus_sms_title"
            android:layout_marginLeft="34dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_cus_sms_title"
            android:textColor="#61607b"
            android:textSize="13.3sp" />

        <Spinner
            android:id="@+id/sp_agency_04_activity_cus_sms"
            android:layout_toRightOf="@+id/tv_agency_04_activity_cus_sms_title"
            android:spinnerMode="dropdown"
            android:layout_marginLeft="10dp"
            android:layout_width="120dp"
            android:layout_height="34dp" />

        <TextView

            android:id="@+id/tv_agency_04_activity_sales_status_title"
            android:layout_toLeftOf="@+id/sp_agency_04_activity_sales_status"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="10dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_sales_status_title"
            android:textColor="#61607b"
            android:textSize="13.3sp" />

        <Spinner
            android:id="@+id/sp_agency_04_activity_sales_status"
            android:layout_marginRight="17dp"
            android:spinnerMode="dropdown"
            android:layout_width="120dp"
            android:layout_height="34dp"
            android:layout_alignParentRight="true"/>









    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34.7dp"
        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp"
        android:gravity="center_horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:background="@color/text_view_header_row_one"
            android:text="@string/agency_04_activity_list_header_type"
            android:gravity="center"
            android:textColor="#a2a0bc"
            android:textSize="15sp" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:background="@color/text_view_header_row_two"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:gravity="center"
            android:text="@string/agency_04_activity_list_header_date"
            android:textColor="#a2a0bc"
            android:textSize="15sp"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:background="@color/text_view_header_row_one"
            android:gravity="center"
            android:text="@string/agency_04_activity_list_header_hope_model"
            android:textColor="#a2a0bc"
            android:textSize="15sp"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="2"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:background="@color/text_view_header_row_two"
            android:gravity="center"
            android:text="@string/agency_04_activity_list_header_facility_code"
            android:textColor="#a2a0bc"
            android:textSize="15sp"/>
    </LinearLayout>


    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="50"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="5.3dp"
        android:paddingTop="3.7dp">

        <ListView
            android:id="@+id/lv_agency_04_transfer_sale_infomation"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipToPadding="false"
            android:groupIndicator="@null"
            android:listSelector="@android:color/transparent"
            android:scrollbars="vertical"
            android:dividerHeight="1px"
            android:divider="#c7c8ca"/>
    </LinearLayout>
    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="기사/상담원 정보"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:textSize="12sp"
            android:textColor="#16133e"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp"
        android:paddingTop="3.7dp">
        <TextView

            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="성명 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>

        <TextView
            android:id="@+id/tv_agency_04_activity_engineer_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/black"/>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="고객정보"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:textSize="12sp"
            android:textColor="#16133e"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="연락처1 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
        <TextView
            android:id="@+id/tv_agency_04_activity_tel_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="연락처2 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
        <TextView
            android:id="@+id/tv_agency_04_activity_s_tel_no"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="상호 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
        <TextView
            android:id="@+id/tv_agency_04_activity_cust_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="주소 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
        <TextView
            android:id="@+id/tv_agency_04_activity_addr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
    </LinearLayout>

    <LinearLayout

        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12.7dp"
        android:layout_marginRight="14dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text="우편번호 : "
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>
        <TextView
            android:id="@+id/tv_agency_04_activity_zip_code"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#81818a"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/black"/>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:visibility="gone">

        <TextView
            android:id="@+id/tv_agency_04_activity_title_sales"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/agency_04_activity_sales_title"
            android:textColor="#61607b"
            android:textSize="13.3sp" />

        <CheckBox
            android:id="@+id/cb_agency_04_activity_sales"
            android:layout_toRightOf="@+id/tv_agency_04_activity_title_sales"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_centerVertical="true"
            android:layout_marginLeft="21.7dp"
            android:background="@drawable/user_id_save_check_box"
            android:button="@null" />

        <Spinner
            android:id="@+id/sp_agency_04_activity"
            android:layout_alignParentRight="true"
            android:layout_marginRight="17dp"
            android:spinnerMode="dropdown"
            android:layout_width="100dp"
            android:layout_height="34dp" />

        <TextView
            android:id="@+id/tv_agency_04_activity_trans_type_date_title"
            android:layout_toLeftOf="@+id/sp_agency_04_activity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="34dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="2019-09-23"
            android:textColor="#61607b"
            android:textSize="13.3sp" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="22.3dp">
        <TextView
            android:id="@+id/tv_aos05_search_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="3"
            android:layout_gravity="center_vertical|left"
            android:paddingLeft="11.3dp"
            android:text=""
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:textColor="#4564e8"
            android:visibility="invisible"/>

        <EditText
            android:id="@+id/et_search_value"
            android:layout_width="0dp"
            android:layout_height="26.3dp"
            android:layout_gravity="center_vertical"
            android:focusable="false"
            android:focusableInTouchMode="true"
            android:layout_weight="6"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="12sp"
            android:background="@color/white"
            android:visibility="invisible"/>


        <RelativeLayout
            android:id = "@+id/btn_aos05_search"
            android:layout_width="70.7dp"
            android:layout_height="26.3dp"
            android:layout_gravity="center_vertical"

            android:background="@drawable/search_btn_selector">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10.7dp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6.2sp"
                android:layout_centerInParent="true"
                android:text="등록"/>
        </RelativeLayout>

    </LinearLayout>
</LinearLayout>