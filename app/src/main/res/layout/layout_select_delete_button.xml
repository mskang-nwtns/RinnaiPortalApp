<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="112.7dp"
    android:layout_height="38.3dp"
    android:background="@drawable/select_delete_bg_normal"
    android:id="@+id/button_bg">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center">
        <ImageView
            android:id="@+id/symbol"
            android:layout_width="14.7dp"
            android:layout_height="15.3dp"
            android:src="@drawable/alldelete_icon_press"/>

        <TextView
            android:id="@+id/tv_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanum_gothic_bold"
            android:layout_marginLeft="7.7dp"
            android:text="선택삭제"
            android:textColor="#ffffff"
            android:textSize="13.3sp" />
    </LinearLayout>

</RelativeLayout>