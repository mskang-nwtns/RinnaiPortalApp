<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/login_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_weight="0.5"
        android:orientation="horizontal">

        <ImageView
            android:layout_width="16.7dp"
            android:layout_height="16.7dp"
            android:layout_marginLeft="12.7dp"
            android:layout_marginTop="25.3dp"
            android:src="@drawable/index_item" />

        <TextView
            android:id="@+id/tv_wms_activity_10_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="22.7dp"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/title_product_move_handling"
            android:textColor="#16133e"
            android:textSize="18.7sp" />

        <ImageView
            android:layout_width="76.7dp"
            android:layout_height="31.3dp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="22.7dp"
            android:layout_marginRight="20.7dp"
            android:src="@drawable/menu_01_normal" />
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="3"
        android:orientation="vertical"
        android:layout_marginLeft="12.7dp"
        android:layout_marginTop="13dp"
        android:layout_marginRight="14dp">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:layout_marginBottom="5dp"
            >
            <RelativeLayout
                android:background="@drawable/layout_out_border"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginRight="10dp"
                android:layout_weight="1">
                <Spinner
                    android:id="@+id/sp_wms_activity_10_from"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:spinnerMode="dropdown" />
            </RelativeLayout>
            <RelativeLayout
                android:background="@drawable/layout_out_border"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginLeft="10dp"
                android:layout_weight="1">
                <Spinner
                    android:id="@+id/sp_wms_activity_10_to"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:spinnerMode="dropdown"/>
            </RelativeLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/layout_out_border">

            <TextView
                android:id="@+id/tv_wms_activity_10_reading_barcode"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="#494762"
                android:textSize="25sp"
                android:singleLine="true"
                android:paddingLeft="5dp"
                android:gravity="center_vertical"
                android:fontFamily="@font/nanum_gothic_extra_bold"/>

        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="4"
            android:layout_marginTop="5dp"
            android:layout_marginBottom="5dp"
            android:background="@drawable/layout_out_border">

            <EditText
                android:id="@+id/tv_wms_activity_10_model_name"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="#00000000"
                android:enabled="false"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:hint="Model Name"
                android:paddingLeft="5dp"
                android:singleLine="true"
                android:text=""
                android:textColor="#494762"
                android:textSize="25sp" />

        </LinearLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="2"
        android:layout_marginLeft="12.7dp"
        android:layout_marginTop="13dp"
        android:layout_marginRight="14dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <RelativeLayout
                android:id = "@+id/btn_wms_activity_submit"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:layout_alignParentLeft="true"
                android:background="@drawable/search_btn_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25sp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="6.2sp"
                    android:layout_centerInParent="true"
                    android:text="발행"/>
            </RelativeLayout>
            <RelativeLayout
                android:id = "@+id/btn_wms_activity_init"
                android:layout_width="80dp"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:layout_alignParentRight="true"
                android:background="@drawable/search_btn_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25sp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="6.2sp"
                    android:layout_centerInParent="true"
                    android:text="등록"/>
            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toRightOf="@+id/btn_wms_activity_submit"
                android:layout_toLeftOf="@+id/btn_wms_activity_init"
                android:layout_marginLeft="12.7dp"
                android:layout_marginRight="14dp"
                android:orientation="vertical">

                <EditText
                    android:id="@+id/tv_wms_activity_10_model_gas_type"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:hint="Gas Type"
                    android:enabled="false"
                    android:textColor="#494762"
                    android:textSize="25sp"
                    android:singleLine="true"
                    android:paddingLeft="5dp"
                    android:gravity="center_vertical"
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:layout_marginBottom="5dp"
                    android:background="@drawable/layout_out_border"/>

                <EditText
                    android:id="@+id/et_wms_activity_10_count"
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_weight="1"
                    android:layout_marginTop="5dp"
                    android:hint="수량"
                    android:inputType="number"
                    android:background="@drawable/layout_out_border"
                    android:gravity="center_vertical|right"
                    android:textColor="#494762"
                    android:textSize="25sp"
                    android:singleLine="true"
                    android:paddingRight="5dp"
                    android:fontFamily="@font/nanum_gothic_extra_bold"/>
            </LinearLayout>

        </RelativeLayout>

    </LinearLayout>



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="5"
        android:orientation="vertical"
        android:layout_marginLeft="12.7dp"
        android:layout_marginTop="13dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="10dp">

        <ListView
            android:id="@+id/lv_read_product_barcode"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="80"
            android:clipToPadding="false"
            android:listSelector="@android:color/transparent"
            android:dividerHeight="1px"
            android:scrollbars="vertical"
            android:divider="#c7c8ca"
            android:layout_marginBottom="10dp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="80dp">

            <RelativeLayout
                android:id = "@+id/btn_wms_activity_clear"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:layout_gravity="center_vertical"
                android:background="@drawable/search_btn_selector"
                android:layout_alignParentLeft="true">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25sp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="6.2sp"
                    android:layout_centerInParent="true"
                    android:text="삭제"/>
            </RelativeLayout>

            <RelativeLayout
                android:id = "@+id/btn_wms_activity_close"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="4"
                android:layout_gravity="center_vertical"
                android:layout_alignParentRight="true"
                android:background="@drawable/search_btn_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="25sp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="6.2sp"
                    android:layout_centerInParent="true"
                    android:text="닫기"/>
            </RelativeLayout>
        </RelativeLayout>

    </LinearLayout>

</LinearLayout>