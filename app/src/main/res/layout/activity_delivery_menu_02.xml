<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/login_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="70dp">

        <ImageView
            android:layout_width="16.7dp"
            android:layout_height="16.7dp"
            android:layout_marginLeft="12.7dp"
            android:layout_marginTop="25.3dp"
            android:src="@drawable/index_item" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="22.7dp"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/delivery_02_activity_title"
            android:textColor="#16133e"
            android:textSize="18.7sp" />



        <!--<Button
            android:id="@+id/btn_calendar"
            android:layout_alignParentRight="true"
            android:layout_width="180dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:text=""/>
        -->
    </RelativeLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="바코드"
            android:textColor="#6f87ee"
            android:layout_marginLeft="35.7dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="15.7sp"/>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_delivery_02_activity_model_barcode"
            android:text=""
            android:textColor="#494762"
            android:layout_marginLeft="35.7dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="18.7sp"/>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/tv_delivery_02_activity_model_name"
                android:text=""
                android:textColor="#494762"
                android:layout_marginLeft="35.7dp"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="18.7sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="41.7dp"
                android:id="@+id/tv_delivery_02_activity_model_gas_type"
                android:text=""
                android:textColor="#494762"
                android:layout_marginLeft="35.7dp"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="18.7sp"/>
        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/line_grey"/>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="작업일시"
            android:textColor="#6f87ee"
            android:layout_marginLeft="35.7dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="15.7sp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_delivery_02_activity_date"
            android:text=""
            android:textColor="#494762"
            android:layout_marginLeft="35.7dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="18.7sp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/line_grey"/>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="작업자"
            android:textColor="#6f87ee"
            android:layout_marginLeft="35.7dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="15.7sp"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_delivery_02_activity_pad_no"
            android:text=""
            android:textColor="#494762"
            android:layout_marginLeft="35.7dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="18.7sp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/line_grey"/>
    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="25dp"
            android:layout_marginTop="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="대리점"
                android:textColor="#6f87ee"
                android:layout_marginLeft="35.7dp"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="15.7sp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_marginRight="41.7dp"
                android:id="@+id/tv_delivery_02_activity_cust_code"
                android:text=""
                android:textColor="#6f87ee"
                android:layout_marginLeft="35.7dp"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="18.7sp"/>
        </RelativeLayout>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/tv_delivery_02_activity_cust_name"
            android:text=""
            android:textColor="#494762"
            android:layout_marginLeft="35.7dp"
            android:layout_marginTop="15dp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="18.7sp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:orientation="horizontal"
        android:layout_marginLeft="34dp"
        android:layout_marginTop="13dp"
        android:background="@color/line_grey"/>

</LinearLayout>