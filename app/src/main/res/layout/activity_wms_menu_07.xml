            android:ems="10"
            android:gravity="center_vertical"
            android:text=""
            android:textColor="#16133e"
            android:textSize="16sp"
            android:paddingLeft="22.7dp"
            android:singleLine="true"/>
        <kr.co.rinnai.dms.common.custom.CustomSelectDeleteButtonView
            android:layout_width="113.3dp"
            android:layout_height="42.3dp"
            android:layout_marginLeft="233.7dp"
            android:clickable="true"
            android:id="@+id/btn_del_selection"
            app:symbol="@drawable/alldelete_icon_press"
            app:pressSymbol="@drawable/alldelete_icon_press"
            android:text="@string/value_del_selection"/>
        <!--
        <Button
            android:layout_width="113.3dp"
            android:layout_height="42.3dp"
            android:layout_marginLeft="233.7dp"
            android:id="@+id/btn_del_selection"
            android:text="@string/value_del_selection"/>
            -->
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:id="@+id/ll_warehouse_activity_07_model_search"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <TextView

            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12.7dp"
            android:layout_centerVertical="true"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/warehouse_07_activity_model_search"
            android:textColor="#61607b"
            android:textSize="13.3sp"
            android:layout_gravity="center_vertical"/>

        <EditText
            android:id="@+id/et_warehouse_07_activity_model_name"
            android:layout_width="0dp"
            android:layout_height="26.3dp"
            android:layout_weight="6"
            android:background="@color/white"
            android:layout_gravity="center_vertical"
            android:layout_marginLeft="20dp"
            android:singleLine="true"
            android:windowSoftInputMode="adjustResize|adjustPan"/>


        <RelativeLayout
            android:id = "@+id/btn_warehouse_07_activity_search"
            android:layout_marginRight="10dp"
            android:layout_width="40.7dp"
            android:layout_height="26.3dp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:background="@drawable/search_btn_selector">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10.7dp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6.2sp"
                android:layout_centerInParent="true"
                android:text="찾기"/>
        </RelativeLayout>


    </LinearLayout>

