            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:layout_marginLeft="19.3dp">

            <kr.co.rinnai.dms.common.custom.CustomButtonView
                android:id="@+id/btn_search_location"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:pressSymbol="@drawable/position_icon"
                app:symbol="@drawable/position_icon"
                android:clickable="true"
                app:text="@string/value_search_location" />
            <!--Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/btn_search_location"
                android:text="@string/value_search_location"/>-->
        </RelativeLayout>

        <RelativeLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:orientation="horizontal" >
            <kr.co.rinnai.dms.common.custom.CustomButtonView
                android:id="@+id/btn_idd_confirmation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:pressSymbol="@drawable/transparent"
                app:symbol="@drawable/transparent"
                android:clickable="true"
                app:text="@string/value_add_product"
                android:visibility="visible"/>
            <!--<Button
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/btn_idd_confirmation"
                android:text="@string/value_confirmation"/> -->
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>