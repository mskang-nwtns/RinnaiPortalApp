<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal" >
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="대리점"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <TextView
            android:id="@+id/tv_agency_07_activity_cust_name"
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="wrap_content"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#494762"
            android:text=""
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:paddingLeft="5dp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="현장명"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <EditText
            android:id="@+id/et_agency_07_activity_site_name"
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="wrap_content"
            android:text=""
            android:textColor="#494762"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:paddingLeft="5dp"
            android:singleLine="true"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal"
        android:layout_marginTop="0dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="총세대수"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>

        <EditText
            android:id="@+id/et_agency_07_activity_total_house_hold_count"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_weight="7"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:inputType="number"
            android:paddingLeft="5dp"
            android:text=""
            android:textColor="#494762"
            android:textSize="16sp"
            android:singleLine="true"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="30"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="주소"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity=""
            android:layout_marginTop="7dp"
            android:textColor="#6f87ee"/>
        <LinearLayout
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:orientation="vertical">
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_weight="10"
                android:layout_height="33dp"
                android:orientation="horizontal">
                <RelativeLayout
                    android:layout_width="200dp"
                    android:layout_weight="10"
                    android:layout_height="33dp"
                    android:layout_centerVertical="true">

                    <TextView
                        android:id="@+id/tv_post_no"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent"
                        android:fontFamily="@font/nanum_gothic_extra_bold"
                        android:paddingLeft="5dp"
                        android:text=""
                        android:textColor="#494762"
                        android:textSize="16sp" />
                </RelativeLayout>
                <RelativeLayout
                    android:id = "@+id/btn_agency_menu_07_activity_address"
                    android:layout_width="100dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dp"
                    android:layout_alignParentRight="true"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:background="@drawable/search_btn_selector">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="10.7dp"
                        android:textColor="#ffffff"
                        android:lineSpacingExtra="6.2sp"
                        android:layout_centerInParent="true"
                        android:text="주소 조회"/>
                </RelativeLayout>

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_weight="10"
                android:layout_height="29.3dp"
                >

                <TextView
                    android:id="@+id/tv_addr1"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center_vertical"
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:paddingLeft="5dp"
                    android:layout_marginTop="5dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="#494762"
                    android:textSize="16sp" />

            </RelativeLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="29.3dp"
                android:layout_weight="10">
                <EditText
                    android:id="@+id/et_addr2"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:paddingLeft="5dp"
                    android:paddingRight="5dp"
                    android:singleLine="true"
                    android:text=""
                    android:textColor="#494762"
                    android:textSize="16sp" >
                </EditText>

            </RelativeLayout>


        </LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="건물형태"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <Spinner
            android:id="@+id/sp_agency_07_activity_building_type"
            android:layout_marginRight="10dp"
            android:spinnerMode="dropdown"
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="난방형태"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <Spinner
            android:id="@+id/sp_agency_07_activity_heating_type"
            android:layout_marginRight="10dp"
            android:spinnerMode="dropdown"
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"/>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="납품업체"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <EditText
            android:id="@+id/et_agency_07_activity_sale_cust"
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="wrap_content"
            android:text=""
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#494762"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:paddingLeft="5dp"
            android:singleLine="true"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="10"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">
        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="납품년월"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity="center"
            android:textColor="#6f87ee"/>
        <RelativeLayout
            android:layout_width="0dp"
            android:layout_weight="7"
            android:layout_height="match_parent"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/tv_agency_07_activity_sale_ym"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:textSize="16sp"
                android:gravity="center"
                android:text=""
                android:textColor="#494762"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:paddingLeft="5dp"/>
            <RelativeLayout
                android:id = "@+id/btn_agency_menu_07_activity_calendar"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="20dp"
                android:layout_alignParentRight="true"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:background="@drawable/search_btn_selector">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="10.7dp"
                    android:textColor="#ffffff"
                    android:lineSpacingExtra="6.2sp"
                    android:layout_centerInParent="true"
                    android:text="달력"/>
            </RelativeLayout>

        </RelativeLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="25"
        android:orientation="horizontal"
        android:layout_marginTop="13dp">

        <TextView
            android:layout_width="0dp"
            android:layout_weight="3"
            android:layout_height="wrap_content"
            android:text="특기사항\n(100자)"
            android:paddingLeft="16.3dp"
            android:textSize="16sp"
            android:layout_gravity=""
            android:layout_marginTop="7dp"
            android:textColor="#6f87ee"/>

        <EditText
            android:id="@+id/et_agency_07_activity_special_note"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="7"
            android:background="@color/white"
            android:paddingLeft="5dp"
            android:paddingTop="5dp"
            android:gravity="left|top"
            android:singleLine="false"
            android:text=""
            android:textColor="#494762"
            android:textSize="16sp"
            android:maxLength="100"/>
    </LinearLayout>
</LinearLayout>