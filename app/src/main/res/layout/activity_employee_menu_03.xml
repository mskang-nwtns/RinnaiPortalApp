<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/login_bg"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="70dp">

        <ImageView
            android:layout_width="16.7dp"
            android:layout_height="16.7dp"
            android:layout_marginLeft="12.7dp"
            android:layout_marginTop="25.3dp"
            android:src="@drawable/index_item" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="34dp"
            android:layout_marginTop="22.7dp"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="@string/employee_03_activity_title"
            android:textColor="#16133e"
            android:textSize="18.7sp" />
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="47dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp">
        <RelativeLayout
            android:id="@+id/rl_agency_03_top_menu_cust"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:background="@drawable/release_top_bg_select">

            <TextView
                android:id="@+id/tv_agency_03_top_menu_cust"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/employee_03_activity_top_menu_01"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="20.5dp"
                android:textColor="#ffffff" />

            <ImageView
                android:id="@+id/iv_agency_03_top_menu_cust_icon"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="10dp"
                android:layout_toRightOf="@+id/tv_agency_03_top_menu_cust"
                android:src="@drawable/tab_focus_item" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_agency_03_top_menu_model"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:background="@drawable/release_top_bg_disable">
            <TextView
                android:id="@+id/tv_agency_03_top_menu_model"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/employee_03_activity_top_menu_02"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="20.5dp"
                android:textColor="#ffffff"/>

            <ImageView
                android:id="@+id/iv_agency_03_top_menu_model_icon"
                android:layout_width="8dp"
                android:layout_height="4.3dp"
                android:layout_toRightOf="@+id/tv_agency_03_top_menu_model"
                android:layout_marginLeft="10dp"
                android:layout_centerVertical="true"
                android:src="@drawable/tab_focus_item"/>

        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/rl_agency_03_top_menu_warehouse"
            android:layout_width="match_parent"
            android:layout_height="47dp"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:visibility="invisible">
            <TextView
                android:id="@+id/tv_agency_03_top_menu_warehouse"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/employee_03_activity_top_menu_03"
                android:fontFamily="@font/nanum_gothic_bold"
                android:textSize="20.5dp"
                android:textColor="#ffffff"/>

            <ImageView
                android:id="@+id/iv_agency_03_top_menu_warehouse_icon"
                android:layout_width="8dp"
                android:layout_height="4.3dp"
                android:layout_toRightOf="@+id/tv_agency_03_top_menu_warehouse"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                android:src="@drawable/tab_focus_item"/>

        </RelativeLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="36dp"
        android:orientation="horizontal"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp"
        android:background="#282645">

        <TextView
            android:id="@+id/tv_search_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|left"
            android:layout_weight="8"
            android:fontFamily="@font/nanum_gothic_bold"
            android:paddingLeft="11.3dp"
            android:text=""
            android:textColor="#aaa6e7"
            android:textSize="12sp"
            android:singleLine="true"
            android:ellipsize="marquee"/>
        <EditText
            android:id="@+id/et_search_value"
            android:layout_width="0dp"
            android:layout_height="26.3dp"
            android:layout_weight="4"
            android:background="@color/white"
            android:layout_gravity="center_vertical"
            android:singleLine="true"
            android:windowSoftInputMode="adjustResize|adjustPan"/>


        <RelativeLayout
            android:id = "@+id/btn_search"
            android:layout_width="40.7dp"
            android:layout_height="26.3dp"
            android:layout_weight="1"
            android:layout_gravity="center_vertical"
            android:background="@drawable/search_btn_selector">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="10.7dp"
                android:textColor="#ffffff"
                android:lineSpacingExtra="6.2sp"
                android:layout_centerInParent="true"
                android:text="찾기"/>
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34.7dp"

        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp"
        android:gravity="center_horizontal">
        <TextView
            android:id="@+id/tv_aos3_top_title_01"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:text="LPG"
            android:gravity="center"
            android:textColor="#a2a0bc"
            android:textSize="15sp" />
        <ImageView
            android:layout_width="14.7dp"
            android:layout_height="8dp"
            android:layout_marginLeft="17dp"
            android:layout_marginTop="20dp"
            android:layout_marginRight="17dp"
            android:src="@drawable/slide_tab_arrow" />

        <TextView
            android:id="@+id/tv_aos3_top_title_02"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_weight="5"
            android:fontFamily="@font/nanum_gothic_extra_bold"
            android:gravity="center"
            android:text="LNG"
            android:textColor="#a2a0bc"
            android:textSize="15sp"/>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="34.7dp"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="14dp"
        android:orientation="horizontal">
        <TextView
            android:id="@+id/tv_search_type"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="3"
            android:background="@color/text_view_header_row_one"
            android:gravity="center"
            android:text="매장명"
            android:textSize="16sp"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textColor="#3d3c49"
            android:lineSpacingExtra="-11sp"/>
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/text_view_header_row_two"
            android:gravity="center"
            android:text="이월"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="16sp"
            android:textColor="#6c66b4"
            android:lineSpacingExtra="-11sp" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/text_view_header_row_one"
            android:gravity="center"
            android:text="입고"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="16sp"
            android:textColor="#3d3c49"
            android:lineSpacingExtra="-11sp" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/text_view_header_row_two"
            android:gravity="center"
            android:text="출고"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="16sp"
            android:textColor="#6c66b4"
            android:lineSpacingExtra="-11sp" />
        <TextView
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:background="@color/text_view_header_row_one"
            android:gravity="center"
            android:text="재고"
            android:fontFamily="@font/nanum_gothic_bold"
            android:textSize="16sp"
            android:textColor="#3d3c49"
            android:lineSpacingExtra="-11sp" />

    </LinearLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/aos_menu03_pager"
        android:layout_width="match_parent"
        android:layout_height="240dp"
        android:layout_weight="40"
        android:layout_marginLeft="14dp"
        android:layout_marginRight="14dp"
        android:layout_marginBottom="23.3dp"
        android:overScrollMode="never" />

</LinearLayout>