<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content" 
    android:orientation="vertical">
    <RelativeLayout
        android:layout_width="350.5dp"
        android:layout_height="40dp"
        android:orientation="horizontal"
        android:gravity="center"
        android:background="@drawable/login_bg">
        <TextView
            android:id="@+id/tv_dia_model_count_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="20sp"
            android:text=""
            android:fontFamily="@font/nanum_gothic_extra_bold"/>
    </RelativeLayout>

    <LinearLayout
        android:layout_width="350.5dp"
        android:layout_height="500dp"
        android:orientation="vertical"
        android:gravity="center"
        android:background="@drawable/login_bg">

        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="80dp"
            android:orientation="horizontal"
            android:gravity="center"
            android:background="@drawable/contents_area02_bg">
            <LinearLayout
                android:id="@+id/ll_product_count"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="80dp"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_product_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:textSize="46.7sp"
                    android:textColor="@color/red" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="80dp"
                android:orientation="horizontal"
                android:gravity="center">
                <TextView
                    android:id="@+id/tv_product_input_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:textSize="46.7sp"
                    android:textColor="#4564e8" />
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="80dp"
            android:layout_marginTop="10dp"
            android:orientation="horizontal"
            android:gravity="center"
            android:id="@+id/ll_cell_make"
            android:background="@drawable/contents_area02_bg"
            android:visibility="visible">
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="80dp"
                android:id="@+id/ll_cell_make_title"
                android:orientation="horizontal"
                android:gravity="center">
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/ck_cell_make"/>
                <TextView
                    android:id="@+id/tv_cell_make"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:text="생산년월"
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:textSize="30.7sp"
                    android:textColor="@color/black" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="80dp"
                android:orientation="horizontal"
                android:gravity="center">
                <LinearLayout
                    android:id="@+id/ll_input_cell_make_year"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <TextView
                        android:id="@+id/tv_input_cell_make_year"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:fontFamily="@font/nanum_gothic_extra_bold"
                        android:textSize="46.7sp"
                        android:textColor="#4564e8" />
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_input_cell_make_dot"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="."
                    android:fontFamily="@font/nanum_gothic_extra_bold"
                    android:textSize="46.7sp"
                    android:textColor="#4564e8" />
                <LinearLayout
                    android:id="@+id/ll_input_cell_make_month"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal"
                    android:gravity="center">
                    <TextView
                        android:id="@+id/tv_input_cell_make_month"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text=""
                        android:fontFamily="@font/nanum_gothic_extra_bold"
                        android:textSize="46.7sp"
                        android:textColor="#4564e8" />
                </LinearLayout>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/ll_count_bookmark"
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:gravity="center"
            android:orientation="horizontal"
            android:paddingTop="10dp">
            <Button
                android:id="@+id/btn_no_24"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:background="@drawable/number_bt_selector"
                android:text="24"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_30"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:background="@drawable/number_bt_selector"
                android:text="30"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_36"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:background="@drawable/number_bt_selector"
                android:text="36"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:gravity="center"
            android:orientation="horizontal">
            <Button
                android:id="@+id/btn_no_1"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="1"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_2"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="2"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_3"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="3"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:gravity="center"
            android:orientation="horizontal">
            <Button
                android:id="@+id/btn_no_4"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="4"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button android:id="@+id/btn_no_5"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="5"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_6"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="6"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:gravity="center"
            android:orientation="horizontal">
            <Button
                android:id="@+id/btn_no_7"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="7"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_8"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="8"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_no_9"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="9"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">
            </Button>
        </LinearLayout>
        <LinearLayout
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:gravity="center"
            android:orientation="horizontal">
            <Button android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="24"
                android:visibility="invisible">

            </Button>
            <Button
                android:id="@+id/btn_no_0"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="0"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">

            </Button>
            <Button
                android:id="@+id/btn_del"
                android:layout_height="match_parent"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:text="지우기"
                android:fontFamily="@font/nanum_gothic_extra_bold"
                android:textSize="20.7sp">
            </Button>
        </LinearLayout>

        <kr.co.rinnai.dms.common.custom.CustomApplyButtonView
            android:id="@+id/btn_confirm"
            android:layout_width="328.5dp"
            android:layout_height="54dp"
            android:text="적용"
            app:pressSymbol="@drawable/apply_icon"
            app:symbol="@drawable/apply_icon" />
    </LinearLayout>
</LinearLayout>